;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Blockade of Gopher Keep"
config/version="0.3.1"
run/main_scene="res://prefabs/game_controller.tscn"
config/features=PackedStringArray("4.3", "C#", "Mobile")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://assets/sprites/splash_screen.png"
boot_splash/fullsize=false
config/icon="res://assets/sprites/icon.png"
config/windows_native_icon="res://assets/sprites/icon.ico"
boot_splash/minimum_display_time=120

[display]

window/size/viewport_width=1280
window/size/viewport_height=1120
window/size/mode=3
window/stretch/mode="canvas_items"
window/stretch/scale=5.0
window/vsync/vsync_mode=2

[dotnet]

project/assembly_name="BlockBreaker"

[editor]

naming/default_signal_callback_name="On{NodeName}{SignalName}"
naming/default_signal_callback_to_self_name="On{SignalName}"

[filesystem]

import/blender/enabled=false
import/fbx/enabled=false

[global]

EncryptionPassword="Gopher's are totally fake you guys!"
DefaultLevelFolder="res://scenes/levels"
DefaultDifficultyFolder="res://assets/text/difficulties"
CustomDifficultyFolder="user://difficulties"
ConfigFilePath="user://settings.cfg"
PatchNotesFilePath="res://assets/text/patchnotes.txt"
DefaultLeaderboardFilePath="res://assets/text/default_leaderboard.txt"
CustomLeaderboardFilePath="user://leaderboard.txt"
SkillIconsFilePath="res://assets/sprites/ui_elements/skill_icons"
DefaultConfigFilePath="res://assets/text/default_settings.cfg"

[global_group]

manual_translation="Elements to translate with value injections."

[gui]

theme/custom="res://theme/ui.theme"
theme/custom_font="res://assets/fonts/Silver_adjusted.tres"
theme/default_font_antialiasing=0

